<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zx.dao.AdminaccountDao">
	<select id="queryAdminaccountByNameAndPwd" resultType="com.zx.beans.AdminAccount" parameterType="String">
		SELECT username, password
		FROM adminAccount
		WHERE
		username = #{username}
		AND
		password = #{password}
	</select>
</mapper>